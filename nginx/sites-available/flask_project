server {
	listen 127.0.0.1:8000;


	
    location / {
        proxy_pass http://127.0.0.1:8000; 
    }
    
    
    
    location /data {
        alias  /home/anna/Pulpit/TIN/flask_env/flask_project/static/;

			location /data/nowe {
			alias /etc/nginx/html/;
		} 
			location /data/data {
			alias /etc/nginx/html/data;
			}
			
			location /data/chat {
			alias /etc/nginx/html/chat;
			}
		
    }

	
}
